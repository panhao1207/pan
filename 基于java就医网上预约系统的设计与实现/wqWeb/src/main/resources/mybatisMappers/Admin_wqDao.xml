<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.Admin_wqDao">
	<!-- 如果返回类型是int  String  list map… 这些类型的时候  用resultType -->
	<select id="login" parameterType="entity.Admin_wqBean" resultMap="admin">
		select * from admin_wq where Abianhao_wq=#{Abianhao_wq} and Apwd_wq=#{Apwd_wq}
	</select>
	<select id="findAdminAll" resultMap="admin">
		select * from admin_wq
	</select>
  
  	<select id="findAdminById" parameterType="Integer" resultMap="admin">
		select * from admin_wq where Aid_wq=#{0} 
	</select>
	
	<select id="findAdminByIds" parameterType="Integer" resultMap="admin">
		select * from admin_wq where Aid_wq=#{0} 
	</select>
  
  <update id="updateAdminInfo" parameterType="entity.Admin_wqBean">
    update admin_wq
    set Aname_wq = #{Aname_wq,jdbcType=VARCHAR},
      Apwd_wq = #{Apwd_wq,jdbcType=VARCHAR},
      AloginTime_wq = #{AloginTime_wq,jdbcType=TIMESTAMP}
    where Aid_wq = #{Aid_wq,jdbcType=INTEGER }
  </update>

  <insert id="addAdminInfo" parameterType="entity.Admin_wqBean">
    insert into admin_wq (Aname_wq, Apwd_wq,AtypeId_wq,AloginTime_wq,Abianhao_wq)
    values (#{Aname_wq,jdbcType=VARCHAR}, #{Apwd_wq,jdbcType=VARCHAR}, #{AtypeId_wq,jdbcType=INTEGER}, #{AloginTime_wq,jdbcType=TIMESTAMP}, #{Abianhao_wq,jdbcType=VARCHAR})
  </insert>

	<resultMap type="entity.Admin_wqBean" id="admin">
		<result column="Aid_wq" jdbcType="INTEGER" property="Aid_wq" />
   	 	<result column="Aname_wq" jdbcType="VARCHAR" property="Aname_wq" />
    	<result column="Apwd_wq" jdbcType="VARCHAR" property="Apwd_wq" />
    	<result column="AtypeId_wq" jdbcType="INTEGER" property="AtypeId_wq" />
    	<result column="AloginTime_wq" jdbcType="TIMESTAMP" property="AloginTime_wq" />
    	<result column="Abianhao_wq" jdbcType="VARCHAR" property="Abianhao_wq" />
	</resultMap>
</mapper>